    <name>KEVN Mobile</name>
    <description>
        A tool for pickup and delivery Kerry Express VN.
    </description>
    <author email="thinh.nguyen@kerryexpres.com.vn" href="https://kes.kerryexpress.com.vn">
        IT Kerry Express VN Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-build-tool" value="gradle" />
        <preference name="android-minSdkVersion" value="19" />
        <allow-intent href="market:*" />
        <preference name="APP_SECRET" value="ac0a1612-59ab-482e-80da-d7014d04a4d4" />
        <preference name="CodePushDeploymentKey" value="DfMgrGWErW59AgYLW-xJCImHYiGGryZjCFy5WV" />
        <preference name="APPCENTER_APP_SECRET" value="ea198278-ae2f-48a2-bdf1-0a4fdac82f59" />
        <resource-file src="google-services.json" target="app/google-services.json" />
        <allow-intent href="market:*" />
        <preference name="APPCENTER_ANALYTICS_ENABLE_IN_JS" value="true" />
        <preference name="APPCENTER_CRASHES_ALWAYS_SEND" value="false" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />  
    </platform>
    <preference name="DisallowOverscroll" value="true" />
